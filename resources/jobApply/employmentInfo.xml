<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.1.7//EN"
    "http://struts.apache.org/dtds/struts-2.1.7.dtd">
<struts>
	<package name="employmentInfo" namespace="/" extends="struts-default">
		<action name="employmentInfo_*" method="{1}" class="cn.hrg.cd.web.action.EmploymentInfoAction">
			<result name="success">/WEB-INF/views/employmentInfo/search.jsp</result>
			<result name="view">/WEB-INF/views/employmentInfo/view.jsp</result>
			<result name="login">login1.html</result>
			<result name="reCreateIndexSuccess">/WEB-INF/views/commons/success.html</result>
			<result name="reCreateIndexError">/WEB-INF/views/commons/error.html</result>
		</action>
	</package>
</struts>
